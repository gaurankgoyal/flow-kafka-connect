{
  "name": "flow-ods-price-book-sink",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "db.LLLSSRS.PRICE_BOOK",
    "connection.url": "${vault:app-flow-kv/data/${env}/db:ods_db_kafka_url:${env}}",
    "connection.user": "${vault:app-flow-kv/data/${env}/db:ods_db_kafka_user:${env}}",
    "connection.password": "${vault:app-flow-kv/data/flowsecretdirectory/${env}/ods/system/rw:mpa_infa:${env}}",
    "table.name.format": "rms_land.price_book",
    "auto.create": "false",
    "insert.mode": "upsert",
    "errors.tolerance": "all",
    "errors.retry.delay.max.ms": "30000",
    "errors.retry.timeout": "120000",
    "errors.deadletterqueue.topic.name": "error.price_book",
    "errors.deadletterqueue.context.headers.enable": "true",
    "errors.log.enable": "true",
    "quote.column.names": "never",
    "quote.sql.identifiers": "never",
    "pk.mode": "record_value",
    "pk.fields": "STYLE_OPTION, LOC, EFF_FROM, EFF_TO",
    "batch.max.rows": "1",
    "transforms": "convert_current_ts,convert_op_ts,processed_time,RenameField,BlacklistField,Cast,convert_etl_insert_ts,convert_etl_update_ts,convert_eff_from,convert_eff_to",
    "transforms.convert_current_ts.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convert_current_ts.target.type": "Timestamp",
    "transforms.convert_current_ts.field": "current_ts",
    "transforms.convert_current_ts.format": "yyyy-MM-dd HH:mm:ss",
    "transforms.convert_op_ts.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convert_op_ts.target.type": "Timestamp",
    "transforms.convert_op_ts.field": "op_ts",
    "transforms.convert_op_ts.format": "yyyy-MM-dd HH:mm:ss",
    "transforms.processed_time.type": "com.lululemon.flow.kafka.connect.transforms.InsertProcessedTimestampField$Value",
    "transforms.processed_time.processedTimestamp.field": "last_update_ts",
    "transforms.RenameField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.RenameField.renames": "op_type:change_indicator, op_ts:source_ts, current_ts:integration_ts, pos:source_sequence",
    "transforms.BlacklistField.type": "org.apache.kafka.connect.transforms.ReplaceField$Value",
    "transforms.BlacklistField.blacklist": "REPLICATION_DATETIME",
    "transforms.Cast.type": "org.apache.kafka.connect.transforms.Cast$Value",
    "transforms.Cast.spec": "source_sequence:int64",
    "transforms.convert_etl_insert_ts.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convert_etl_insert_ts.target.type": "Timestamp",
    "transforms.convert_etl_insert_ts.field": "ETL_INSERT_TS",
    "transforms.convert_etl_insert_ts.format": "yyyy-MM-dd HH:mm:ss",
    "transforms.convert_etl_update_ts.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convert_etl_update_ts.target.type": "Timestamp",
    "transforms.convert_etl_update_ts.field": "ETL_UPDATE_TS",
    "transforms.convert_etl_update_ts.format": "yyyy-MM-dd HH:mm:ss",
    "transforms.convert_eff_from.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convert_eff_from.target.type": "Timestamp",
    "transforms.convert_eff_from.field": "EFF_FROM",
    "transforms.convert_eff_from.format": "yyyy-MM-dd",
    "transforms.convert_eff_to.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
    "transforms.convert_eff_to.target.type": "Timestamp",
    "transforms.convert_eff_to.field": "EFF_TO",
    "transforms.convert_eff_to.format": "yyyy-MM-dd"
  }
}
